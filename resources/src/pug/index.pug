extends /templates/_app

block config
  - const _PATH = "./"
  - const _PAGENAME = "home"

block meta
  +meta({
    title: "ミッドセンチュリー家具ブランド｜TIME｜Japan",
    description: "『TIME-タイム-』は、20世紀半ば、いわゆるミッドセンチュリーテイストデザインのシリーズです。すべて日本国内で製造し、つくり手の卓越された技術によって、耐久性をもたせ、何世代にも渡り楽しんでいただける、高品質な家具です。",
  })

block content
  include /components/include/_header
  include /components/include/_openNav

  div.mv.swiper-container.u_anime-fade(data-slider="mvSlider" data-anime="addClass")
    div.mv_list.swiper-wrapper
      div.mv_list-item.swiper-slide
        +img(`${_RESOURCES}assets/images/index/mv_1.jpg`, `time`)
      div.mv_list-item.swiper-slide
        +img(`${_RESOURCES}assets/images/index/mv_2.jpg`, `time`)
      div.mv_list-item.swiper-slide
        +img(`${_RESOURCES}assets/images/index/mv_3.jpg`, `time`)
      div.mv_list-item.swiper-slide
        +img(`${_RESOURCES}assets/images/index/mv_4.jpg`, `time`)
      div.mv_list-item.swiper-slide
        +img(`${_RESOURCES}assets/images/index/mv_5.jpg`, `time`)

